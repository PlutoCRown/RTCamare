{
  "name": "rtc-native-server",
  "version": "1.0.0",
  "private": true,
  "main": "dist/server/index.js",
  "license": "MIT",
  "scripts": {
    "start": "bun dist/server/index.js",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && rsbuild dev",
    "dev:backend": "bun --watch server/index.js",
    "build": "npm run build:backend && npm run build:frontend",
    "build:frontend": "cd frontend && rsbuild build",
    "build:backend": "rspack build",
    "build:prod": "NODE_ENV=production npm run build"
  },
  "dependencies": {
    "qrcode": "^1.5.4",
    "selfsigned": "^2.4.1",
    "stun": "^2.1.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@rsbuild/core": "^1.7.2",
    "@rsbuild/plugin-react": "^1.4.2",
    "@rspack/cli": "^1.7.1",
    "@rspack/core": "^1.7.1",
    "@tanstack/react-router": "^1.147.3",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "concurrently": "^9.2.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "typescript": "^5.9.3"
  }
}